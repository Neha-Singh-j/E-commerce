{
  "version": 2,
  "builds": [
    {
      "src": "app-vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app-vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app-vercel.js": {
      "maxDuration": 30
    }
  }
} 